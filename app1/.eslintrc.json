{
    "settings": {
       "react": {
        "version": "detect"
       }
    }, 
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["db.js", "*.test.js"],
    "rules": {
        "indent": ["error", 2]
    }
}
